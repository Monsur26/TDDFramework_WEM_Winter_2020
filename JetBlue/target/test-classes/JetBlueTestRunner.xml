<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="JetBlue Automation Script" parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <!--<parameter name="cloudEnvName" value="saucelab"/>-->
    <parameter name="os" value="windows"/>
    <!--    <parameter name="os" value="OS X"/>-->
    <parameter name="os_version" value="10"/>
    <!--        <parameter name="os_version" value="Sierra"/>-->
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="83"/>
    <parameter name="url" value="https://www.jetblue.com"/>





    <test name="HomePage">
        <classes>
            <class name="hometest.PageTest1"></class>
            <class name="hometest.PageTest2"></class>
            <class name="hometest.PageTest3"></class>
            <class name="hometest.PageTest4"></class>
            <class name="hometest.PageTest5"></class>
            <class name="hometest.PageTest6"></class>







        </classes>

    </test>




</suite>








