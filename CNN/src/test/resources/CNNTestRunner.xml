<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="CNN Automation Script" parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <!--<parameter name="cloudEnvName" value="saucelab"/>-->
    <parameter name="os" value="windows"/>
    <!--    <parameter name="os" value="OS X"/>-->
    <parameter name="os_version" value="10"/>
    <!--        <parameter name="os_version" value="Sierra"/>-->
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="83"/>
    <parameter name="url" value="https://www.CNN.com"/>





    <test name="HomePage">
        <classes>


            <class name="hometest.CNNTest1"></class>
            <class name="hometest.CNNTest2"></class>
            <class name="hometest.CNNTest3"></class>
            <class name="hometest.CNNTest4"></class>
            <class name="hometest.CNNTest5"></class>
            <class name="hometest.CNNTest6"></class>
            <class name="hometest.CNNTest7"></class>
            <class name="hometest.CNNTest8"></class>
            <class name="hometest.CNNTest9"></class>
            <class name="hometest.CNNTest10"></class>


        </classes>

    </test>



<!---->

</suite>








