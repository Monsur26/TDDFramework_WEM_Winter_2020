<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="CNN Automation Script" parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <!--<parameter name="cloudEnvName" value="saucelab"/>-->
    <parameter name="os" value="windows"/>
    <!--    <parameter name="os" value="OS X"/>-->
    <parameter name="os_version" value="10"/>
    <!--        <parameter name="os_version" value="Sierra"/>-->
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="83"/>
    <parameter name="url" value="https://www.CNN.com"/>





    <test name="HomePage">
        <classes>
                <class name="hometest.HomeTest1"></class>
                <class name="hometest.Hometest2"></class>
                <class name="hometest.HomeTest3"></class>
                <class name="hometest.HomeTest4"></class>
                <class name="hometest.HomeTest5"></class>
                <class name="hometest.HomeTest6"></class>
                <class name="hometest.HomeTest7"></class>





        </classes>

    </test>




</suite>








